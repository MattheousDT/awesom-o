            const stats = new XMLHttpRequest();
            stats.open("GET", `/api/v3/stats`, true);
            stats.onload = e => {
                if (stats.readyState === 4 && stats.status === 200) {
                    const statsyay = JSON.parse(stats.response).stats;
                    $("#usersplaceholder").html(statsyay.members.total);
                    $("#serversplaceholder").html(statsyay.servers);
                    $("#uptimeplaceholder").html(Math.round(statsyay.status.uptime));
                }
            };
            stats.send();

